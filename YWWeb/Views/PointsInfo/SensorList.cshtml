@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>传感器列表</title>
    @Html.Partial("~/Views/Home/BaseHTML.cshtml")
    <script src="~/Content/UserJS/lbywfw._jm.js" type="text/javascript"></script>
</head>
<body>
    <table id="list_data" style="width: auto; height: 780px; width: 100%" idfield='TagID'
     pagination="true" data-options="iconCls:'icon-save',rownumbers:true,pageSize:50,pageList:[10,20,50],toolbar:'#tb',method:'get',striped:true"
     fitcolumns="true" fit="true">
        <thead>
            <tr>
                <th field='TagID'  checkbox="true">
                </th>
                <th field='PName' width="100px">
                    站室名称
                </th>
                <th field='DeviceName' width="100px">
                    设备名称
                </th>
                <th field='Position' width="100px">
                    监测位置
                </th>
                 <th field='pAddr' width="200px">
                    地址信息
                </th>
                <th field='传感器SN编码' width="100px">
                    传感器编号
                </th>
                <th field='DevName' width="100px">
                    传感器名称
                </th>
                <th field='DevModel' width="100px">
                    传感器型号
                </th>
                <th field='InstallTime' width="100px" data-options="formatter: DateFormat">
                    投运日期
                </th>
                <th field='Enable' width="100px" data-options="formatter: DeviceUseState">
                    使用状态
                </th> 

            </tr>
        </thead>
    </table>


    <div id="tb" class="searchbar">
        <div class="searchrow">
            设备名称:
            <select id="SDID" style="width: 150px; height: 26px;">
            </select>


          @*  传感器型号:
            <input id="TagName" />*@


            监测位置:
            <input id="Poison" />
        </div>
        <div id="userbutton" class="searchbutton">
        </div>
    </div>

   @* <div id="editwin" title="编辑工单信息" class="easyui-window" closed="true" style="width: 1000px;
        height: 600px; padding: 5px;" minimizable="false" maximizable="false" collapsible="false">
    </div>

    <div id="detailwin" title="查看工单详情" class="easyui-window" closed="true" style="width: 1000px;
        height: 600px; padding: 5px;" minimizable="false" maximizable="false" collapsible="false">
    </div>

    <div id="show" style="display: none; z-index: 999; height: auto; width: 750px; top: 10px;
        left: 100px; position: absolute; border: 2px  solid #e8f0ff;">
    </div>*@

    <script src="~/Content/UserJS/SensorList_jm.js"></script>
    <script type="text/javascript" src="~/Content/UserJS/ResizeList._jm.js"></script>
</body>
</html>
