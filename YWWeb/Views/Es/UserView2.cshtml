@{
    ViewBag.Title = "UserView";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>合同管理</title>
    @Html.Partial("~/Views/Home/BaseHTML.cshtml")
    <style type="text/css">
        .bt1 {
            color: red;
        }

        .closepic {
            float: right;
            width: 105px;
            right: 20px;
            top: 120px;
        }

        #table1 tr:nth-child(even) {
            background: #d9edf7;
        }

        #table1 tr td:first-child {
            width: 90px;
        }
    </style>
    <script src="~/Content/js/echarts.min3.js" type="text/javascript"></script>
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css" />
    <script src="~/Content/js/bootstrap.min.js"></script>
</head>
<body>
    <div class="container" id="contid" style="display:none;">
        <h1 id="pCompanyName" style="text-align:center;"></h1>
        <div class="row clearfix">
            <div class="col-md-6 column" style="margin-top:50px;">
                <table class="table" id="table1">
                    <tbody>
                        <tr>
                            <td>
                                从事行业：
                            </td>
                            <td>
                                <div id="pPosition"> </div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                装机容量：
                            </td>
                            <td>
                                <div id="pLinkMan"> </div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                容需计价方式：
                            </td>
                            <td>
                                <div id="pMobile"> </div>
                            </td>
                        </tr>


                        <tr>
                            <td>
                                政府目录电价：
                            </td>
                            <td>
                                <div id="pAreaID"> </div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                偏差承担方式：
                            </td>
                            <td>
                                <div id="hangye"> </div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                联系人：
                            </td>
                            <td>
                                <div id="InstalledCapacity"></div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                手机：
                            </td>
                            <td>
                                <div id="EleCalWay"></div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                电话：
                            </td>
                            <td>
                                <div id="GovEleLevel"></div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                电压等级：
                            </td>
                            <td>
                                <div id="dianya"></div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                用电类别：
                            </td>
                            <td>
                                <div id="leibie"></div>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                地址：
                            </td>
                            <td>
                                <div id="dizhi"></div>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div class="col-md-6 column" style="text-align:center;margin-top:50px;">
                <div class="row clearfix">
                    <table border="1" @*class="easyui-dialog"*@ @*closed="true"*@ style="width: 550px; height: 200px; padding: 5px;"
                           minimizable="false" maximizable="false" collapsible="false">
                        <tbody id="conS"></tbody>
                    </table>

                </div>
                <div class="row clearfix" style="margin-top:50px;">
                    <div>
                        <div id="userCharts1" class="powerquality_curve   lineloss_curve" style="height:200px;">
                        </div>
                        <h4>本月电量(MH·H)</h4>
                    </div>
                    @*<div class="col-md-6 column">
                            <div id="userCharts2" class="powerquality_curve   lineloss_curve" style="height:200px;">
                            </div>
                            <h4>本月电费(元)</h4>
                        </div>*@
                </div>

            </div>
        </div>
        <div>
            <p style="text-align:center; font-size:30px;font-weight:bold;">配电室</p>
            @*<div id="content" style="margin-top:20px;"></div>*@
            <div class="row clearfix" id="content">
            </div>
        </div>
    </div>
</body>
</html>
<script type="text/javascript">
        var id=@Html.Raw(Request.QueryString["id"]);
</script>
<script src="~/Content/UserJS/UserView._jm.js"></script>