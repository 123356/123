@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>合同编辑</title>
    <style type="text/css">
        .divicons {
            float: left;
            border: solid 1px #ccc;
            margin: 5px;
            padding: 5px;
            text-align: center;
            cursor: pointer;
        }

            .divicons:hover {
                color: #FFF;
                border: solid 1px #3399dd;
                background: #2288cc;
                filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#33bbee', endColorstr='#2288cc');
                background: linear-gradient(top, #33bbee, #2288cc);
                background: -moz-linear-gradient(top, #33bbee, #2288cc);
                background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#33bbee), to(#2288cc));
                text-shadow: -1px -1px 1px #1c6a9e;
                font-weight: bold;
            }
    </style>
</head>
<body>
    <table cellpadding="" cellspacing="" border="0" class="d_list">
        <tr>
            <td class="d_l_t" style="width: 80px;text-align:left;padding-left:20px;">
                合同名称：
            </td>
            <td class="d_l_d">
                <input id="CtrName" runat="server" name="CtrName" maxlength="20"
                       class="easyui-validatebox" style="width: 200px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
                <input id="id" type="hidden" />
            </td>
            <td class="d_l_t" style="text-align:left;padding-left:20px;">
                合同编号：
            </td>
            <td class="d_l_d">
                <input id="ConNo" runat="server" name="CtrName" maxlength="20"
                        class="easyui-validatebox" style="width: 210px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
            </td>
        </tr>
        <tr>
            @*<td class="d_l_t" style="text-align:left;padding-left:20px;">
                类型：
            </td>
            <td class="d_l_d">
                <select id="CtrPType" runat="server" name="CtrName" maxlength="20"
                        class="textbox-text validatebox-text validatebox-readonly" style="width: 210px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
            </td>*@
            <td class="d_l_t" style="text-align:left;padding-left:20px;" id="mingcheng">
                客户名称：
            </td>
            <td class="d_l_d" id="inputName">
                <select id="CtrPName" runat="server" name="CtrName" maxlength="20"
                        class="textbox-text validatebox-text validatebox-readonly" style="width: 210px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
                <input id="id" type="hidden" />
            </td>
            <td class="d_l_t" style="text-align:left;padding-left:20px;" id="mingcheng1" hidden>
                发电厂调度名称：
            </td>
            <td class="d_l_d" id="inputName1" hidden>
                <input id="CtrAdmin" runat="server" name="CtrName" maxlength="20"
                       class="easyui-validatebox" style="width: 200px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
                <input id="id" type="hidden" />
            </td>

        </tr>
        <tr>
            <td class="d_l_t" style="text-align:left;padding-left:20px;">
                负责人员：
            </td>
            <td class="d_l_d">
                <select id="person" runat="server" name="CtrName" maxlength="20"
                        class="textbox-text validatebox-text validatebox-readonly" style="width: 210px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
            </td>
            <td class="d_l_t" style="text-align:left;padding-left:20px;">
                联系人：
            </td>
            <td class="d_l_d">
                <input id="LinkMan" runat="server" name="CtrName" maxlength="20"
                       class="easyui-validatebox" style="width: 200px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
                <input id="id" type="hidden" />
            </td>
           
        </tr>
        <tr>
            <td class="d_l_t" style="text-align:left;padding-left:20px;">
                联系电话：
            </td>
            <td class="d_l_d">
                <input id="Tel" runat="server" name="CtrName" maxlength="20"
                       class="easyui-validatebox" style="width: 200px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
            </td>
            <td class="d_l_t" style="text-align:left;padding-left:20px;">
                开始日期：
            </td>
            <td class="d_l_d">
                <input id="start_time" runat="server" name="CtrName" maxlength="20"
                       class="easyui-datebox" style="width: 210px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
                <input id="id" type="hidden" />
            </td>
          
        </tr>
        <tr>
            <td class="d_l_t" style="text-align:left;padding-left:20px;">
                结束日期：
            </td>
            <td class="d_l_d">
                <input id="end_time" runat="server" name="CtrName" maxlength="20"
                       class="easyui-datebox" style="width: 210px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
                <input id="id" type="hidden" />
            </td>
        </tr>
        <tr>
            <td class="d_l_t" style="text-align:left;padding-left:20px;">事项：</td>
            <td colspan="3">
                <table cellpadding="" cellspacing="" border="0" class="d_list">
                    <tr>
                        <td class="d_l_t" style="width: 80px;text-align:left;padding-left:20px;">
                            事项：
                        </td>
                        <td class="d_l_d">
                            <input id="shixiang" runat="server" name="CtrName" maxlength="20"
                                   class="easyui-validatebox" style="width: 200px; height: 24px;" />@*<b class="RStar">&nbsp*</b>*@
                                <input id="id" type="hidden" />
                            </td>
                            <td class="d_l_t" style="width: 80px;text-align:left;padding-left:20px;">
                                开始时间：
                            </td>
                            <td class="d_l_d">
                                <input id="start_shixiang" runat="server" name="CtrName" maxlength="20"
                                       class="easyui-datebox" style="width: 210px; height: 24px;" />@*<b class="RStar">&nbsp*</b>*@
                                    <input id="id" type="hidden" />
                                </td>
                            </tr>
                            <tr>
                                <td class="d_l_t" style="width: 80px;text-align:left;padding-left:20px;">
                                    人员：
                                </td>
                                <td class="d_l_d">
                                    <select id="person1" runat="server" name="CtrName" maxlength="20"
                                            class="textbox-text validatebox-text validatebox-readonly" style="width: 210px; height: 24px;" />@*<b class="RStar">&nbsp*</b>*@
                                        </td>
                                        <td class="d_l_t" style="width: 80px;text-align:left;padding-left:20px;">
                                            提前提醒(天)：
                                        </td>
                                        <td class="d_l_d">
                                            <input id="numTemplate" class="easyui-numberspinner" style="width:80px;"
                                                   required="required" data-options="min:0,max:100,editable:true">
                                            <a id="btnAddTemplate" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-add'">添加</a>
                                            <a id="btnDelTemplate" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-cancel'">删除</a>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td colspan="4">
                                            <table id="dgTemplateNameList" class="easyui-datagrid" style="height: 222px; width: 97%;" @*data-options="
                                   method:'get',
                                   showGroup:true,
                                   scrollbarSize:0,
                                   fitColumns:true,
                                   striped:true,
                                   rownumbers:false
                                   "*@>
                                                <thead>
                                                    <tr>
                                                        <th data-options="field:'ID',hidden:true">项编号</th>
                                                        <th data-options="field:'Name',width:120">项名称</th>
                                                        <th data-options="field:'StartTime',width:120" formatter="formatDate">开始时间</th>
                                                        <th data-options="field:'UserName',width:120">人员</th>
                                                        <th data-options="field:'IsOk',width:120">是否完成</th>
                                                        <th data-options="field:'Remark',width:120">反馈信息</th>
                                                        <th data-options="field:'PersonID',hidden:true">人员id</th>
                                                        <th data-options="field:'BeforDay',width:140">提前天数</th>
                                                </thead>
                                            </table>
                                        </td>
                                    </tr>
                                </table>
                            </td>
                            @*<td></td>
                                <td></td>*@
                        </tr>
                        <tr>
                            <td class="d_l_t" colspan="4" style="text-align:left;padding-left:20px;background-color:#EFEDF2">
                                合同备注
                            </td>
                        </tr>
                        <tr>
                            <td class="d_l_t" colspan="4" style="height:60px; text-align: left;padding-left:20px;background-color:#EFEDF2">
                                <textarea id="CtrInfo" class="easyui-validatebox validatebox-text" style="height:60px;  width: 97%;"></textarea>
                            </td>
                        </tr>

                        <tr>
                            <td class="d_l_t" colspan="4" id="listOthers" style="padding-left:20px;background-color:#EFEDF2"></td>
                        </tr>
                    </table>
                    <div style="text-align: center; padding-top: 20px;">
                        <a href="javascript:;" id="btnSave" class="table_btn btn_style radius5" data-options="iconCls:'icon-ok'">
                            保存
                        </a> <a href="javascript:;" class="table_btn1 btn_style radius5" data-options="iconCls:'icon-cancel'"
                                onclick="$('#editwin').window('close');">关闭</a>
                    </div>
                    <div id="editWin" class="easyui-dialog" style="width: 800px; height: auto; padding: 10px 20px;
        left: 850px; top: 60px; max-height: 550px;" data-options="closed:true,buttons:'#dlg-buttons'"
                         title="用户Logo列表">
                        <div class="div-body" id="icon_data">
                        </div>
                    </div>
                    <script type="text/javascript">
                        var id=@Html.Raw(Request.QueryString["id"]);
                        var type=@Html.Raw(Request.QueryString["type"]);
                    </script>
    <script src="~/Content/UserJS/ConstractEdit._jm.js"></script>

                </body>
            </html>
