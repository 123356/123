@{
    ViewBag.Title = "RoomDataMonitoring";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />
    <title>科室数据监测</title>
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/iview/dist/styles/iview.css?v=1.01">
    <link rel="stylesheet" type="text/css" href="~/Content/css/EnergyDfficiency.css?v=1.09" />
</head>
<body>
    <div id="app" style="padding:0">


        <div class="left flex-item">
            <div class="module-title">
                <img src="~/Content/images/energyDifficiency/fxjg.png" class="icon" />
                <Dropdown placement="top-start" trigger="click">
                    <span class="dropdownTitle">
                        新源医院能源结构
                        <Icon type="md-arrow-dropdown" size="25" />
                    </span>
                    <dropdown-menu slot="list">
                        <dropdown-item>楼</dropdown-item>
                        <dropdown-item>科室</dropdown-item>
                        <dropdown-item>房间</dropdown-item>

                    </dropdown-menu>
                </Dropdown>
                <!--<span>能源分项解构</span>-->
                <!-- <button class="btn">加入对比</button>-->
            </div>
            <div class="treeList">
                <Tree
                      :data="treeData"
                      show-checkbox 
                      empty-text="暂无数据"
                      class="tree"
                      v-on:on-select-change="selectChange"></Tree>
            </div>
        </div>


        <div class="right allMain departmentWatch">
            <iframe id="departFrame" name="departFrame" :src="departFrameSrc" frameborder="0"></iframe>
        </div>

    </div>



    <script type="text/javascript" src="~/Content/js/jquery-1.12.2.min.js"></script>
    <script src="~/Content/js/echarts.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Content/js/jquery.cookie.js"></script>
    <script type="text/javascript" src="~/Content/js/vue.min.js"></script>
    <script type="text/javascript" src="~/Content/js/vue-resource.min.js"></script>
    <script type="text/javascript" src="~/Content/iview/dist/iview.js"></script>
    <script type="text/javascript" src="~/Content/UserJS/DepartDataMonitoring._zc.js?v=1.09"></script>
</body>
</html>

