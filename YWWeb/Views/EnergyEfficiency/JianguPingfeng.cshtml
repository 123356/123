@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>StationRoom</title>
    @Html.Partial("~/Views/Home/BaseHTML.cshtml")
    <script src="~/Content/UserJS/menucontrol._jm.js" type="text/javascript"></script>
    <script src="~/Content/js/echarts.min3.js" type="text/javascript"></script>
</head>
<body>
<div class="jangupf">
    <div id="Histb" class="searchbar searchbar2">
        <div class="searchrow">
                设备名称:
                <select id="SPID" style="width: 150px; height: 26px;">
                </select>
        </div>
        <div class="searchrow">
@*            <button title="测 试" onclick="test()" class="page_table_but"><img src="/Content/images/Icon16/zoom.png">测 试</button><div id = "test">1111</div>
*@            
            基准日期:
            <input type="radio" class="GraphType" name="GraphType" value="144" onclick="DoSearch()" checked="checked" />按日
            <input type="radio" class="GraphType" name="GraphType" value="616" onclick="DoSearch()" />按月
            <input type="radio" class="GraphType" name="GraphType" value="707" onclick="FadeIn()" />自定义
        </div>
            <div class="searchbutton" id="scroll" style="display: none; width: 600px;">
                <input id="StartDate" class="easyui-datebox" style="width: 150px; height: 26px;" />
                至
                <input id="EndDate" class="easyui-datebox" style="width: 150px; height: 26px;" />
                <button title="查 询" onclick="DoSearch()" class="page_table_but"><img src="/Content/images/Icon16/zoom.png">查 询</button>
            </div>
    </div>
    <div class="jangupftop">
        <h3 class="current_selection" id ="cob"></h3>    
        <div class="jangupf_charts1" id="userCharts1"></div>
    </div>
    <div class="jangupfbottom">
        <div class="jangupf_charts2" id="userCharts2"></div>
        <div class="jangupf_charts3" id="userCharts3"></div>
        <div class="clear"></div>
        <div id="" class="jangupf_tab">
                <table width="99%" border="0" align="center" cellpadding="0" cellspacing="0" class="page_table_list scroll_bar">
                 <thead>
                    <tr>
                        <th height="35" scope="col">
                            日期
                        </th>
                        <th scope="col">总量</th>
                        <th scope="col">尖</th>
                        <th scope="col">峰</th>
                        <th scope="col">平</th>
                        <th scope="col">谷</th>
                    </tr>
                    </thead>
                    <tbody id = "list">
                    </tbody>
				</table>
            </div>
    </div>
</div>
</body>
</html>
<script src="~/Content/UserJS/JianguPingfeng._jm.js"></script>