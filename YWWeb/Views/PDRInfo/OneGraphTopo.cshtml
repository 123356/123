@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="content-type" content="text/html;charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>新版本一次图</title>
    <link rel="stylesheet" href="~/Content//topo/css/rescss.css">
    <link rel="stylesheet" href="~/Content//topo/css/topo.css">
    <script src="~/Content//topo/js/mqtt.js"></script>

 
</head>
<body>
    <div id="pid" style="display:none">@ViewData["pid"]</div>
    <div id="orderNo" style="display:none">@ViewData["orderNo"]</div>
     <div id="topo"></div>
     <table id="electricMeterBounced">
        <thead>
            <tr>
                <th colspan="4" class='title'></th>
            </tr>
        </thead>
        <tr class="td3 tdb ">
            <th class="b3"></th>
            <th>A</th>
            <th>B</th>
            <th>C</th>
        </tr>
        <tr class="td3 b1">
            <td class="b3">电压(kV)</td>
            <td class="UAA">--</td>
            <td class="UAB">--</td>
            <td class="UAC">--</td>
        </tr>
        <tr class="td3 b1">
            <td class="b3">电流(A)</td>
            <td class="IA">--</td>
            <td class="IB">--</td>
            <td class="IC">--</td>
        </tr>
        <tr class="b1">
            <td class="">功率因数</td>
            <td colspan="3" class="F">--</td>
        </tr>
        <tr>
            <td>总有功功率(kW)</td>
            <td colspan="3" class="P">--</td>
        </tr>
        <tr>
            <td>总无功功率(kVar)</td>
            <td colspan="3" class="Q">--</td>
        </tr>
        <tr class="border b1">
            <td>总有功电度(kWh)</td>
            <td colspan="3" class="K">--</td>
        </tr>
    </table>
    <div class="switchBtn"></div>
    <script src="~/Content//topo/js/palette_config.js"></script>
    <script src="~/Content//topo/js/jquery.js"></script>
    <script src="~/Content//topo//js/jtopo.js"></script>
    <script src="~/Content//topo/js/topo.js"></script>


</body>
</html>
