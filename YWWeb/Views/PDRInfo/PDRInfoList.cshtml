@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>站室管理</title>

    @Html.Partial("~/Views/Home/BaseHTML.cshtml")
    <script src="~/Content/uplaodfy/jquery.uploadifive.js" type="text/javascript"></script>
    <link href="~/Content/uplaodfy/uploadifive.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <table id="list_data" style="width: auto; height: 780px; width: 100%"
           idfield="PID" pagination="true" data-options="iconCls:'icon-save',rownumbers:true,pageSize:50,pageList:[10,20,50],toolbar:'#tb',method:'get',striped:true"
           fitcolumns="true" fit="true">
        <thead>
            <tr>
                <th field=' ' checkbox="true">
                </th>
                <th data-options="field:'PID',width:50">
                    站编号
                </th>
                @* <th field='EadoCode' width="80px">
                        站室编码
                    </th>*@
                <th field='Name' width="80px">
                    站室名称
                </th>
                <th field='CompanyName' width="100px">
                    公司名称
                </th>
                <th field='Position' width="150px">
                    位置
                </th>
                <th field='OperationMan' width="60px">
                    运维联系人
                </th>
                <th field='OperationTel' width="100px">
                    运维联系电话
                </th>
                <th data-options="field:'UseState',width:100,formatter:loadUseState">
                    使用状态
                </th>
                <th data-options="field:'AreaName',width:100@*,formatter:loadAreaName*@">
                    所属区域
                </th>
                <th data-options="field:'TypeName',width:100@*,formatter:loadTypeName*@">
                    站室分类
                </th>
                <th field='FamilyID' width="150px">
                    户号
                </th>
                <th field='CBPeriodBegin' width="150px">
                    抄表日
                </th>
                <th data-options="field:'IndName',width:100@*,formatter:loadElecIndustryName*@">
                    电价行业类型
                </th>
                <th data-options="field:'VName',width:100@*,formatter:loadElecVoltageName*@">
                    电价电压等级
                </th>
                <th data-options="field:'VoltageName',width:100@*,formatter:loadVoltagesName*@">
                    电压等级
                </th>
                <th field='ApplcationTime' width="150px">
                    投运时间
                </th>
            </tr>
        </thead>
    </table>
    <div id="tb" class="searchbar">
        <div class="searchrow">
            站室名称:
            <input id="pname" />
            公司名称:
            <input id="cname" />
        </div>
        <div id="userbutton" class="searchbutton">
        </div>
    </div>
    <div id="editwin" title="编辑站室信息" class="easyui-dialog" closed="true" style="width: 980px;
        height: 600px; padding: 5px;" minimizable="false" maximizable="false" collapsible="false">
        <table cellpadding="" cellspacing="" border="0" class="d_list">
            <tr>
                <td class="d_l_t">
                    站室名称：
                </td>
                <td class="d_l_d">
                    <input id="Name" runat="server" name="Name" missingmessage="站室名称必填项！" class="easyui-validatebox"
                           style="width: 160px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
                    <input id="PID" type="hidden" />
                </td>
                <td class="d_l_t">
                    站室编码：
                </td>
                <td class="d_l_d">
                    <span id="EadoCode"></span>
                </td>
                <td class="d_l_t">
                    所属区域：
                </td>
                <td class="d_l_d">
                    <input class="easyui-combotree" style="width: 160px; height: 26px;" id="AreaID" /><b class="RStar">&nbsp*</b>
                </td>
            </tr>
            <tr>
                @*<td class="d_l_t">
                    联系人：
                </td>
                <td class="d_l_d">
                    <input id="LinkMan" runat="server" name="LinkMan" missingmessage="联系人必填项！" class="easyui-validatebox"
                           style="width: 160px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
                </td>*@
                <td class="d_l_t">
                    公司名称：
                </td>
                <td class="d_l_d">
                    <input class="easyui-combotree" style="width: 180px; height: 26px;" id="CompanyName" />
                    <b class="RStar">&nbsp*</b>
                </td>
                @*<td class="d_l_t">
                    手机号码：
                </td>
                <td class="d_l_d">
                    <input id="Mobile" runat="server" name="Mobile" missingmessage="手机号码必填项！" class="easyui-validatebox"
                           style="width: 160px;" data-options="required:true" onkeyup='this.value=this.value.replace(/\D/gi,"")' />
                    <b class="RStar">&nbsp*</b>
                </td>*@
                <td class="d_l_t">
                    运维联系人：
                </td>
                <td class="d_l_d">
                    <input id="OperationMan" runat="server" name="LinkMan" missingmessage="联系人必填项！" class="easyui-validatebox"
                           style="width: 160px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
                </td>
                <td class="d_l_t">
                    运维联系电话：
                </td>
                <td class="d_l_d">
                    <input id="OperationTel" runat="server" name="Mobile" missingmessage="手机号码必填项！" class="easyui-validatebox"
                           style="width: 160px;" data-options="required:true" onkeyup='this.value=this.value.replace(/\D/gi,"")' />
                    <b class="RStar">&nbsp*</b>
                </td>
            </tr>

            <tr>

                <td class="d_l_t">
                    位置：
                </td>
                <td class="d_l_d">
                    <input id="Position" runat="server" name="Position" missingmessage="位置必填项！" class="easyui-validatebox"
                           style="width: 160px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
                </td>
                <td class="d_l_t">
                    站室分类：
                </td>
                <td class="d_l_d">
                    <select id="TypeID" class="easyui-combobox" itemindex="0" style="width: 120px; height: 26px;"></select><b class="RStar">&nbsp*</b>
                </td>
                <td class="d_l_t">
                    电压等级：
                </td>
                <td class="d_l_d">
                    <input class="easyui-combotree" style="width: 160px; height: 26px;" missingmessage="电压等级必填项！" id="VoltageID" />
                    <b class="RStar">&nbsp*</b>
                </td>


            </tr>

            <tr id="one">

                <td class="d_l_t">
                    投运时间：
                </td>
                <td class="d_l_d">
                    <input class="easyui-datebox" style="width: 160px; height: 26px;" missingmessage="投运时间必填项！" id="ApplcationTime" />
                    <b class="RStar">&nbsp*</b>
                </td>
                <td class="d_l_t">
                    地图坐标：
                </td>
                <td class="d_l_d" colspan="5">
                    <input id="Coordination" runat="server" name="Coordination" missingmessage="地图坐标必填项！"
                           class="easyui-validatebox" style="width: 200px;" data-options="required:true" />
                    <a href="~/Content/GetMap/getMapPointXY.html" target="_blank" class="table_btn1 btn_style radius5">
                        &nbsp;取坐标&nbsp;
                    </a> <b class="RStar">&nbsp*</b>（如：113.191932|22.232813）
                </td>
            </tr>
            <tr>
                @*<td class="d_l_t">
                        APP上传：
                    </td>
                    <td class="d_l_d" colspan="3">
                        <input id="AppCoordination" runat="server" name="AppCoordination" class="easyui-validatebox"
                            disabled="disabled" style="width: 200px;" />
                        <a class="table_btn1 btn_style radius5" id="btn">&nbsp;采用坐标&nbsp;</a>
                    </td>*@
            </tr>
            <tr>
                <td class="d_l_t">
                    上传站室图片：
                </td>
                <td class="d_l_d" colspan="5">
                    <input id="file_upload" class="fileupload" name="file_upload" type="file" multiple="multiple" /><b>最多上传3张,上传图片不能超过1M。</b>
                    <div id="fileQueue" class="fileQueue">
                    </div>

                </td>
            </tr>
            <tr>
                <td class="d_l_t">
                    使用状态：
                </td>
                <td class="d_l_d" colspan="5">
                    <select id="UseState">
                        <option value="0">正常使用</option>
                        <option value="1">暂停使用</option>
                    </select>
                </td>
            </tr>
            <tr>
                <td class="d_l_t" rowspan="2" colspan="2">
                    <p style="text-align:center;font-size:22px;">售电所需信息：</p>
                </td>
                <td class="d_l_t">
                    户号：
                </td>
                <td class="d_l_d">
                    <input id="FamilyID" runat="server" name="FamilyID" missingmessage="户号必填项！" class="easyui-validatebox"
                           style="width: 160px;" data-options="required:true" />
                </td>
                <td class="d_l_t">
                    电价行业类型：
                </td>
                <td class="d_l_d">
                    <input class="easyui-combotree" style="width:160px; height:26px;" missingmessage="电价行业类型必填项！" id="IndID" />
                </td>
            </tr>
            <tr id="two">
                <td class="d_l_t">
                    电价电压等级：
                </td>
                <td class="d_l_d">
                    <input class="easyui-combotree" style="width: 160px; height: 26px;" missingmessage="电价电压等级必填项！" id="VID" />
                </td>
                <td class="d_l_t">
                    抄表日：
                </td>
                <td class="d_l_d">
                    @*<input id="CBPeriodBegin" name="CBPeriodBegin" class="easyui-datebox" style="width:180px;" /><b class="RStar">&nbsp*</b>*@
                    <select id="CBPeriodBegin" class="easyui-combobox" missingmessage="抄表日必填项！" style="width:160px;"></select>
                </td>
            </tr>
            <tr>
                <td class="d_l_t">
                    备注：
                </td>
                <td class="d_l_d" colspan="5">
                    <textarea id="Remarks" class="easyui-validatebox" style="height: 100px; width: 90%;"></textarea>
                </td>
            </tr>
        </table>
        <div style="text-align: center;">
            <a href="javascript:;" id="btnSave" onclick="save()" class="table_btn btn_style radius5"
               data-options="iconCls:'icon-ok'">保存</a> <a href="javascript:;" class="table_btn1 btn_style radius5"
                                                          data-options="iconCls:'icon-cancel'" onclick="$('#editwin').dialog('close');">关闭</a>
        </div>
    </div>
    <script src="~/Content/UserJS/PDRInfoList_jm.js"></script>
    <script type="text/javascript" src="~/Content/UserJS/ResizeList._jm.js"></script>
</body>
</html>
