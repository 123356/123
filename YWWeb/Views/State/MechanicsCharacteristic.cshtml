        $('.curve_switch a').removeClass('curve_switchactive');
        $(this).addClass('curve_switchactive');
    });
    $(function () {
        loadDeviceInfo();
    });
    function loadDataGrid() {
        $('#list_data').datagrid({
            url: '/State/PointsInfoData?rom=' + Math.random(),
            queryParams: { "rows": 20, "page": 1, "did": did, "tagname": "", "Position": "" },
            singleSelect: true,
            height: 700
        });
    }
   
</script>
