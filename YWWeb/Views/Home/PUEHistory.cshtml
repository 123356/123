@{
    ViewBag.Title = "PUEHistory";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />
    <title>历史PUE</title>
    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="~/Content/iview/dist/styles/iview.css?v=1.01">
    <link rel="stylesheet" type="text/css" href="~/Content/css/computerRoomManage.css?v=1.01">
</head>
<body>
    <div id="app" class="history">
        <div class="flex flex-1  flex-module">
            <div class="flex flex-1">
                <ul class="headerUl">
                    <li>数据中心选择：</li>
                    <li>
                        <i-select>
                            <Tree :data="treeData"
                                  empty-text="暂无数据"
                                  :multiple="false"
                                  class="tree"
                                  ref="tree"
                                  :render="renderContent"
                                  v-on:on-select-change="selectChange"></Tree>
                        </i-select>
                    </li>
                    <li>PUE趋势图：</li>
                    <li>
                        <i-select class="typeSelect" v-model="curType">
                            <i-option :name="item.id" :value="item.id" v-for="item in typeList">{{item.name}}</i-option>
                        </i-select>
                    </li>
                    <li>
                        <date-picker type="daterange" placement="bottom-end"  style="width: 200px" v-show="curType==3"></date-picker>
                    </li>
                </ul>
                        
                    
                    
                   
                </div>

            <div class="bottomChart">
                <div class="chart" id="lineChart" style="width:100%;height:100%"></div>
            </div>

        </div>

    </div>

    <script type="text/javascript" src="~/Content/js/jquery-1.12.2.min.js"></script>
    <script src="~/Content/js/echarts.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Content/js/jquery.cookie.js"></script>
    <script type="text/javascript" src="~/Content/js/vue.min.js"></script>
    <script type="text/javascript" src="~/Content/js/vue-resource.min.js"></script>
    <script type="text/javascript" src="~/Content/iview/dist/iview.js"></script>
    <script type="text/javascript" src="~/Content/UserJS/PUEHistory._zc.js?v=1.02"></script>

</body>
</html>

