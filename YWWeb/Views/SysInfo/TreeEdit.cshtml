@{
    Layout = null;
}
 
<!DOCTYPE html>
<html lang="en">
 
<head>
    <meta http-equiv="content-type" content="text/html;charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>通讯编辑</title>

    <link rel="stylesheet" href="~/Content/css/bootstrap.min.css" />
    <link rel="stylesheet" href="//unpkg.com/iview/dist/styles/iview.css">
    @*<link rel="stylesheet" type="text/css" href="~/Content/iview/dist/styles/iview.css?v=1.01">*@
    <script type="text/javascript" src="~/Content/js/vue.min.js"></script>
    <script type="text/javascript" src="~/Content/iview/dist/iview.js"></script>

    <script src="~/Content/topo/js/tree/tree.config.js"></script>
    <script src="~/Content/topo/js/jtopo.js"></script>
    <script type="text/javascript" src="~/Content/js/vue-resource.min.js"></script>
    <style scoped>
        .demo-split {
            height: 800px;
            border: 1px solid #dcdee2;
        }

        .demo-split-pane {
            padding: 10px;
        }

            .demo-split-pane.no-padding {
                height: 200px;
                padding: 0;
            }

        html, body {
            width: 100%;
            height: 100%;
            overflow: hidden;
        }

        #app {
            height: 100%;
        }

        .ivu-layout-sider-children {
            background: #fff;
        }

        .ivu-layout, .layout {
            height: 100%;
        }

        header {
            height: 45px;
            line-height: 45px;
            font-size: 18px;
            padding-left: 30px;
            border-bottom: 1px solid #ddd;
            background: #F5F7F9;
        }

        .iconlist {
            width: 200px;
            padding-right: 20px;
            list-style: none;
        }

            .iconlist li {
                display: inline-block;
                margin: 10px;
            }

            .iconlist img {
                width: 40px;
                height: 40px;
                margin-bottom: 5px;
            }

    
        .ivu-icon:before {
            font-size: 18px;
        }
    </style>
</head>
 
<body>

    1111111111111111111
    <div id="app">
        <template>
            <div class="layout" >
                <template>
                    <div class="layout">
                        <Layout>
                            <Sider hide-trigger  :style="left.style">
                                <header>图形库</header>
                                <template>
                                    <Collapse active-key="1">
                                        <Panel v-for="(item,index) in left.nodeIcon" :key="index">
                                            {{item.name}}
                                            <ul slot="content" class="iconlist">@*overflow-y:auto;height:;*@
                                                <li v-for="a in item.items">
                                                    <img :src="'../../../Content/topo/img/communication/'+a.src" :alt="a.name" class="img-thumbnail">
                                                    <p class="text-center">{{a.name}}</p>
                                                </li>
                                            </ul>
                                        </Panel>
                                    </Collapse>
                                </template>
                            </Sider>
                            <Layout>
                                <Header>
                                    <template>

                                        <Button-group v-for="(itme,index) in center.top">
                                            <i-button type="ghost" :icon=" itme.icon" :style="{'color':'#000','padding':'0','margin':'0px 5px','width':'30px'}" :title="itme.title" :id="itme.name"></i-button>
                                        </Button-group>

                                    </template>
                                </Header>
                                <Content>Content</Content>
                            </Layout>
                            <Sider hide-trigger>
                                <header> 属性</header>
                                <i-form class="form-horizontal" :style="{'height':'95%','overflow-y':'auto','width':'110%','padding-right':'10%'}">
                                    <template>
                                        <table class="table table-bordered">
                                            <thead>
                                                <tr>
                                                    <th>key</th>
                                                    <th>value</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <tr v-for="(itme,index) in right.attributes" :key="index" v-show="itme.sort.indexOf(center.sort)>-1">
                                                    <td style="width:70px;vertical-align:middle">{{itme.name}}</td>

                                                    <td v-if="itme.element=='button'">
                                                        <i-button bind:click="handleClick">确定</i-button>
                                                        @*<i-button bind:click="" type="primary">联通两边按钮</i-button>*@
                                                    </td>
                                                    <td v-else-if="itme.element=='checkbox'">
                                                        <i-input :type="itme.type" :id="itme.title">
                                                    </td>
                                                    <td v-else="">
                                                        <i-input :type="itme.type" :id="itme.title">
                                                    </td>

                                                </tr>
                                            </tbody>
                                        </table>
                                        <br>
                                    </template>
                                </i-form>
                            </Sider>
                        </Layout>
                    </div>
                </template>
            </div>
        </template>
    </div>





        <script>
            var vm = new Vue({
                el: "#app",
                data: {
                    left: {
                        nodeIcon: icon_config,
                        url: "",
                        style: { 'width': '350px', 'overflow':'hidden'}
                    },
                    right: {
                        attributes: palette_Attributes
                    },
                    center: {
                        top: palette_MenuBar,
                        sort:"123"

                    }
                  
                }
            })
        </script>
</body>
 
</html>
</body>
 
</html>
