@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>统计报警设置</title>
    @Html.Partial("~/Views/Home/BaseHTML.cshtml")
    <script src="~/Content/uplaodfy/jquery.uploadifive.js" type="text/javascript"></script>
    <link href="~/Content/uplaodfy/uploadifive.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <table id="list_data" class="easyui-datagrid" style="width: auto;" idfield="TagID"
           pagination="true" data-options="iconCls:'icon-save',rownumbers:true,pageSize:50,pageList:[10,20,50],toolbar:'#tb',method:'post',striped:true"
           fitcolumns="true" fit="true" singleselect="true">
        <thead>
            <tr>
                <th field=' ' checkbox="true"> </th>
                <th data-options="field:'PID',width:80">
                    站室编号
                </th>
                <th data-options="field:'TypeName',width:80,editor:'text'" 
                    editor="{type:'combobox',options:{valueField:'ABCID',textField:'ABCName', url: '/BaseInfo/BindABCPhase?row=' + Math.random(),required:true }}">
                    类型名称
                </th>
                <th data-options="field:'LimitH1',width:50,editor:'numberbox'">
                    高报1
                </th>
                <th data-options="field:'LimitH2',width:50,editor:'numberbox'" >
                    高报2
                </th>
                <th data-options="field:'LimitH3',width:50,editor:'numberbox'" >
                    高报3
                </th>
                <th data-options="field:'LimitL1',width:50,editor:'numberbox'">
                    低报1
                </th>
                <th data-options="field:'LimitL2',width:50,editor:'numberbox'">
                    低报2
                </th>
                <th data-options="field:'LimitL3',width:50,editor:'numberbox'">
                    低报3
                </th>

            </tr>
        </thead>
    </table>
    <form method="post" action="" id="searchform">
        <div id="tb" class="searchbar">
            <div class="searchrow">
                站室：<select id="SPID" style="width: 200px; height: 26px;"></select>
          
            </div>
            <div id="userbutton" class="searchbutton">

            </div>
            <div id="uploadwin" title="导入测点表" class="easyui-dialog" closed="true" style="width: 600px;
        height: 300px; padding: 20px;" minimizable="false" maximizable="false" collapsible="false">
                <table cellpadding="" cellspacing="" border="0" class="d_list">
                    <tr>
                        <td class="d_l_t">
                            上传资料：
                        </td>
                        <td class="d_l_d" colspan="3">
                            <input id="file_upload" class="fileupload" name="file_upload" type="file" multiple="multiple" />
                            <div id="fileQueue" class="fileQueue">
                            </div>
                            <div id="div_files">
                            </div>
                            <div style="display: none" id="kill">
                                114514
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td class="d_l_t"></td>
                        <td class="d_l_d" colspan="3">
                            <a title="导 入" onclick="ImportFile();" class="table_btn1 btn_style radius5">导 入</a>
                            <input type="checkbox" id="Cover" />覆盖导入
                        </td>
                    </tr>
                </table>
                <div id="msg"></div>
            </div>
    </form>
  
    <script type="text/javascript" src="~/Content/UserJS/ErrorPue._zc.js"></script>
    <script src="~/Content/UserJS/lbyw-base._jm.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/Content/UserJS/ResizeList._jm.js"></script>




    @*<script type="text/javascript" src="~/Content/UserJS/ErrorPue._jm.js"></script>*@

</body>
</html>
