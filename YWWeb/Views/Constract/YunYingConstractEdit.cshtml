@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>合同编辑</title>
    <script src="~/Content/timeline/vis.js"></script>
    <script src="~/Content/uplaodfy/jquery.uploadifive.js" type="text/javascript"></script>
    <link href="~/Content/uplaodfy/uploadifive.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/timeline/vis-timeline-graph2d.min.css" rel="stylesheet" type="text/css" />
    <style type="text/css">
        .divicons {
            float: left;
            border: solid 1px #ccc;
            margin: 5px;
            padding: 5px;
            text-align: center;
            cursor: pointer;
        }

            .divicons:hover {
                color: #FFF;
                border: solid 1px #3399dd;
                background: #2288cc;
                filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#33bbee', endColorstr='#2288cc');
                background: linear-gradient(top, #33bbee, #2288cc);
                background: -moz-linear-gradient(top, #33bbee, #2288cc);
                background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#33bbee), to(#2288cc));
                text-shadow: -1px -1px 1px #1c6a9e;
                font-weight: bold;
            }
    </style>
</head>
<body>
    <table cellpadding="" cellspacing="" border="0" class="d_list">
        <tr>
            <td class="d_l_t" style="width: 80px;text-align:left;padding-left:20px;">
                项目名称：
            </td>
            <td class="d_l_d" >
                <input id="ProjectName" runat="server" name="CtrName" maxlength="20"
                       class="easyui-validatebox" style="width: 200px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
            </td>
            <td class="d_l_t" style="text-align:left;padding-left:20px;">
            类型：
        </td>
        <td class="d_l_d">
            <select id="CtrPType" runat="server" name="CtrName" maxlength="20"
                    class="textbox-text validatebox-text validatebox-readonly" style="width: 210px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
        </td>
            <td class="d_l_t" style="width: 80px;text-align:left;padding-left:20px;">
                合同名称：
            </td>
            <td class="d_l_d">
                <input id="CtrName" runat="server" name="CtrName" maxlength="20"
                       class="easyui-validatebox" style="width: 200px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
                <input id="id" type="hidden" />
            </td>
        </tr>
        <tr>
            <td class="d_l_t" style="text-align:left;padding-left:20px;" id="mingcheng">
                客户名称：
            </td>
            <td class="d_l_d" id="inputName">
                <select id="CtrPName" runat="server" name="CtrName" maxlength="20"
                        class="textbox-text validatebox-text validatebox-readonly" style="width: 210px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
                <input id="id" type="hidden" />
            </td>
            <td class="d_l_t" style="text-align:left;padding-left:20px;">
                项目负责人：
            </td>
            <td class="d_l_d">
                <select id="person" runat="server" name="CtrName" maxlength="20"
                        class="textbox-text validatebox-text validatebox-readonly" style="width: 210px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
            </td>
            <td class="d_l_t" style="text-align:left;padding-left:20px;">
                抄送人员：
            </td>
            <td class="d_l_d">
                <select id="persons" runat="server" name="CtrName" maxlength="20"
                        class="textbox-text validatebox-text validatebox-readonly" style="width: 210px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
            </td>       
        </tr>
        <tr>
            <td class="d_l_t" style="text-align:left;padding-left:20px;">
                客户联系人：
            </td>
            <td class="d_l_d">
                <input id="LinkMan" runat="server" name="CtrName" maxlength="20"
                       class="easyui-validatebox" style="width: 200px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
                <input id="id" type="hidden" />
            </td>
            <td class="d_l_t" style="text-align:left;padding-left:20px;">
                客户联系电话：
            </td>
            <td class="d_l_d">
                <input id="Tel" runat="server" name="CtrName" maxlength="20"
                       class="easyui-validatebox" style="width: 200px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
            </td>
            <td class="d_l_t" style="text-align:left;padding-left:20px;">
                所属省份：
            </td>
            <td class="d_l_d">
                <input class="easyui-combotree" style="width: 200px; height: 26px;" id="UnitProvince" />
                <b class="RStar">&nbsp*</b>
            </td>
           
        </tr>
        <tr>
            <td class="d_l_t" style="text-align:left;padding-left:20px;">
                所属地区：
            </td>
            <td class="d_l_d">
                <input class="easyui-combotree" style="width: 200px; height: 26px;" id="UnitCity" />
                <b class="RStar">&nbsp*</b>
            </td>
            <td class="d_l_t" style="text-align:left;padding-left:20px;">
                开始日期：
            </td>
            <td class="d_l_d">
                <input id="start_time" runat="server" name="CtrName" maxlength="20"
                       class="easyui-datebox" style="width: 210px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
                <input id="id" type="hidden" />
            </td>
            <td class="d_l_t" style="text-align:left;padding-left:20px;">
                结束日期：
            </td>
            <td class="d_l_d">
                <input id="end_time" runat="server" name="CtrName" maxlength="20"
                       class="easyui-datebox" style="width: 210px; height: 24px;" data-options="required:true" /><b class="RStar">&nbsp*</b>
                <input id="id" type="hidden" />
            </td>
        </tr>
        <tr>
            <td class="d_l_t" style="text-align:left;padding-left:20px;">事项：</td>
            <td colspan="5">
                <table cellpadding="" cellspacing="" border="0" class="d_list">
                    <tr>
                        <td class="d_l_t" style="width: 80px;text-align:left;padding-left:20px;">
                            事项名称：
                        </td>
                        <td class="d_l_d">
                            <select id="defTemp" runat="server" name="CtrName" maxlength="20"
                                    class="textbox-text validatebox-text validatebox-readonly" style="width: 210px; height: 24px;" data-options="required:true" />

                            <input id="id" type="hidden" />
                        </td>
                        <td class="d_l_t" style="width: 80px;text-align:left;padding-left:20px;">
                            其他事项名称：
                        </td>
                        <td class="d_l_d">
                            <input id="shixiang" runat="server" name="CtrName" maxlength="20"
                                   class="easyui-validatebox" style="width: 200px; height: 24px;" disabled />
                        </td>

                    </tr>
                    <tr>
                        <td class="d_l_t" style="width: 80px;text-align:left;padding-left:20px;">
                            开始时间：
                        </td>
                        <td class="d_l_d">
                            <input id="start_shixiang" runat="server" name="CtrName" maxlength="20"
                                   class="easyui-datebox" style="width: 210px; height: 24px;" />
                            <input id="id" type="hidden" />
                        </td>
                        <td class="d_l_t" style="width: 80px;text-align:left;padding-left:20px;">
                            结束时间：
                        </td>
                        <td class="d_l_d">
                            <input id="end_shixiang" runat="server" name="CtrName" maxlength="20"
                                    class="easyui-datebox" style="width: 210px; height: 24px;" />
                        </td>

                    </tr>
                    <tr>
                        <td class="d_l_t" style="width: 80px;text-align:left;padding-left:20px;">
                            事项负责人员：
                        </td>
                        <td class="d_l_d">
                            <select id="person1" runat="server" name="CtrName" maxlength="20"
                                    class="textbox-text validatebox-text validatebox-readonly" style="width: 210px; height: 24px;" />
                        </td>
                        <td class="d_l_t" style="width: 80px;text-align:left;padding-left:20px;">
                            提前提醒(天)：
                        </td>
                        <td class="d_l_d">
                            <input id="numTemplate" class="easyui-numberspinner" style="width:80px;"
                                   required="required" data-options="min:0,max:100,editable:true">
                            <a id="btnAddTemplate" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-add'">添加</a>
                            <a id="btnDelTemplate" href="#" class="easyui-linkbutton" data-options="iconCls:'icon-cancel'">删除</a>
                        </td>
                    </tr>
                    <tr>
                        <td colspan="6">
                            <table id="dgTemplateNameList" class="easyui-datagrid" style="height: 222px; width: 100%;" @*data-options="
                                   method:'get',
                                   showGroup:true,
                                   scrollbarSize:0,
                                   fitColumns:true,
                                   striped:true,
                                   rownumbers:false
                                   "*@
                                   data-options="singleSelect:true"
                                   >
                                <thead>
                                    <tr>
                                        <th data-options="field:'ID',hidden:true">事项编号</th>
                                        <th data-options="field:'Name'" style="width:22%">事项名称</th>
                                        <th data-options="field:'StartTime'" style="width:12%" formatter="formatDate">开始时间</th>
                                        <th data-options="field:'EndTime'" style="width:12%" formatter="formatDate">结束时间</th>
                                        <th data-options="field:'UserName'" style="width:12%">事项负责人员</th>
                                        <th data-options="field:'IsOk'" style="width:12%" formatter="setState">是否完成</th>
                                        <th data-options="field:'Remark'" style="width:20%" formatter="setRemarks">反馈信息</th>
                                        <th data-options="field:'PersonID',hidden:true">人员id</th>
                                        <th data-options="field:'BeforDay'" style="width:10%">提前天数</th>
                                        <th data-options="field:'DefID',hidden:true">人员id</th>
                                </thead>
                            </table>
                        </td>
                    </tr>
                    <tr id="Okbtn" style="display:none">
                        <td colspan="6" style="text-align:center;">
                            <a id="btnAddTemplate" href="#" class="easyui-linkbutton" onclick="UpdateIsOk();" data-options="iconCls:'icon-save'">确认完成</a>
                        </td>
                    </tr>
                </table>
            </td>
            @*<td></td>
                <td></td>*@
        </tr>
        <tr>
            <td class="d_l_t" colspan="6" style="text-align:left;padding-left:20px;background-color:#EFEDF2">
                合同备注
            </td>
        </tr>
        @*<tr id="upFile">
            <td class="d_l_t">
                上传函件：
            </td>
            <td class="d_l_d">
                <input id="file_upload1" class="fileupload" name="file_upload1" type="file" multiple="multiple" />
                <div id="fileQueue1" class="fileQueue">
                </div>
                <div id="div_files">
                </div>
                <input hidden id="upload1"/>
                <input hidden id="file1" />
            </td>
            <td class="d_l_t">
