            <tr>
                <td class="d_l_t">
                    上传签单：
                </td>
                <td class="d_l_d" colspan="3">
                    <input id="file_upload8" class="fileupload" name="file_upload8" type="file" multiple="multiple" />
                    <div id="fileQueue8" class="fileQueue">
                    </div>
                    <div id="div_files1">
                    </div>
                </td>
            </tr>
            <tr>
                <td class="d_l_t">
                    上传认证资料：
                </td>
                <td class="d_l_d" colspan="3">
                    <input id="file_upload7" class="fileupload" name="file_upload7" type="file" multiple="multiple" />
                    <div id="fileQueue7" class="fileQueue">
                    </div>
                    <div id="div_files">
                    </div>
                </td>
            </tr>
        </table>
        <a href="javascript:$('#file_upload7').uploadifive('upload');$('#file_upload8').uploadifive('upload')" style="font-size: 15px;width:100px;margin: 0 auto;display:block;margin-top:30px;margin-bottom:20px;">开始上传</a>
    </div>


</body>
</html>
