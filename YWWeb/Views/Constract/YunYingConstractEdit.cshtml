            </div>
            </td>
            <td class="d_l_t">
                上传采购合同：
            </td>
            <td class="d_l_d">
                <input id="file_upload3" class="fileupload" name="file_upload3" type="file" multiple="multiple" />
                <div id="fileQueue3" class="fileQueue">
                </div>

                <input hidden id="upload3" />
                <input hidden id="file3" />
                <div id="div_files">
            </div>
            </td>
        </tr>*@
        <tr>
            <td class="d_l_t" colspan="6" style="height:60px; text-align: left;padding-left:20px;background-color:#EFEDF2">
                <textarea id="CtrInfo" class="easyui-validatebox validatebox-text" style="height:60px;  width: 97%;"></textarea>
            </td>
        </tr>
        @*<tr id="DownConTemp" style="display:none;">
            <td class="d_l_t" colspan="6" id="listOthers" style="padding-left:20px;background-color:#EFEDF2;text-align:center;">
            <p style="color:blue;cursor:pointer;" onclick="DownLoadTemp(1)">物资采购合同模板</p>
            <p style="color:blue;cursor:pointer;" onclick="DownLoadTemp(2)">建设工程承包合同模板</p>
            </td>
        </tr>*@
        <tr>
            <td class="d_l_t" colspan="6" id="listOthers" style="padding-left:20px;background-color:#EFEDF2"></td>
        </tr>
    </table>
    <div style="text-align: center; padding-top: 20px;">
        <a href="javascript:;" id="btnSave" class="table_btn btn_style radius5" data-options="iconCls:'icon-ok'">
            保存
        </a> <a href="javascript:;" class="table_btn1 btn_style radius5" data-options="iconCls:'icon-cancel'"
                onclick="$('#editwin').window('close');">关闭</a>
    </div>
    <div id="editWin" class="easyui-dialog" style="width: 800px; height: auto; padding: 10px 20px;
        left: 850px; top: 60px; max-height: 550px;" data-options="closed:true,buttons:'#dlg-buttons'"
         title="用户Logo列表">
        <div class="div-body" id="icon_data">
        </div>
    </div>
    <script type="text/javascript">
        var id=@Html.Raw(Request.QueryString["id"]);
    </script>
    <script src="~/Content/UserJS/YunYingConstractEdit._jm.js"></script>
