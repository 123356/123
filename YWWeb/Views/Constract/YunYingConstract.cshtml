@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>项目管理</title>
    @Html.Partial("~/Views/Home/BaseHTML.cshtml")
    <script src="~/Content/uplaodfy/jquery.uploadifive.js" type="text/javascript"></script>
    <link href="~/Content/uplaodfy/uploadifive.css" rel="stylesheet" type="text/css" />
    <script src="~/Content/timeline/vis.js"></script>
    <link href="~/Content/timeline/vis-timeline-graph2d.min.css" rel="stylesheet" type="text/css" />
    <style type="text/css">
        .bt1 {
            color: red;
        }

        .closepic {
            float: right;
            width: 105px;
            right: 20px;
            top: 120px;
        }
    </style>
</head>
<body>
    <table id="list_data" style="width: auto; height: 780px;" idfield="id"
           pagination="true" data-options="iconCls:'icon-save',rownumbers:true,pageSize:50,pageList:[10,20,50],toolbar:'#tb',method:'get',striped:true"
           fitcolumns="true" fit="true">
        <thead>
            <tr>
                <th field='UserID' checkbox="true">
                </th>
                <th data-options="field:'UID',hidden:true"></th>
                <th data-options="field:'id',hidden:true"></th>
                <th data-options="field:'IsAlarm',hidden:true"></th>
                <th field='ConNo' width="90px">
                    合同编号
                </th>
                <th field='ProjectName' width="90px">
                    项目名称
                </th>
                <th field='CtrPName' width="100px">
                    客户名称
                </th>
                <th field='person' width="100px">
                    负责人员
                </th>
                <th field='CtrName' width="150px">
                    合同名称
                </th>
                <th field='LinkMan' width="100px">
                    客户联系人
                </th>
                <th field='Tel' width="100px">
                    客户联系电话
                </th>
                <th field='start_time' width="100px">
                    开始日期
                </th>
                <th field='end_time' width="100px">
                    结束日期
                </th>
                
                <th field='Type' width="100px" formatter="setType">
                    合同类型
                </th>
              
                <th field='createDate' width="90px">
                    创建时间
                </th>
            </tr>
        </thead>
    </table>
    <div id="tb" class="searchbar">
        <div class="searchrow">
            合同名称:
            <input id="cname" />
        </div>
        <div id="userbuttonThis" class="searchbutton">
            <button title="查 询" onclick="dosearch();" class="page_table_but"><img src="/Content/images/Icon16/zoom.png">查 询</button>
        </div>
        <div id=userbuttonThis" class="searchbutton">
            <button title="新 增" onclick="add();" class="page_table_but">
                <img src="/Content/images/Icon16/add.png">新 增
            </button>
        </div>
        <div id="userbuttonThis" class="searchbutton">
            <button title="删 除" onclick="delect();" class="page_table_but">
                <img src="/Content/images/Icon16/delete.png">删 除
            </button>
        </div>
        <div id="userbuttonThis" class="searchbutton">
            <button title="编 辑" onclick="edit();" class="page_table_but">
                <img src="/Content/images/Icon16/edit.png">编 辑
            </button>
        </div>
        @*<div id="userbuttonThis" class="searchbutton">
            <button title="添加合同函件" onclick="addfujian();" class="page_table_but">
                <img src="/Content/images/Icon16/edit.png">添加合同函件
            </button>
        </div>*@
        <div id="userbuttonThis" class="searchbutton">
            <button title="查看合同详情" onclick="LookDetail();" class="page_table_but">
                <img src="/Content/images/Icon16/edit.png">查看合同详情
            </button>
        </div>
    </div>
    <script src="~/Content/UserJS/YunYingConstract._jm.js"></script>
    <script type="text/javascript" src="~/Content/UserJS/ResizeList._jm.js"></script>
    <div id="editwin" class="easyui-window" closed="true" style="width: 1100px;
        height: 810px; padding: 5px;" minimizable="false" maximizable="false" collapsible="false">
    </div>
    <div id="editwin1" title="添加合同附件" class="easyui-dialog" closed="true" style="width: 600px;
        height: 300px; padding: 20px;" minimizable="false" maximizable="false" collapsible="false">
        <table cellpadding="" cellspacing="" border="0" class="d_list">
            <tr>
                <td class="d_l_t">
                    合同编号：
                </td>
                <td class="d_l_d" colspan="3">
                    <input data-options="lines:true" style="width: 200px;" id="ConNoNo" disabled />
                    <input id="id" type="hidden" />
                </td>
            </tr>
            <tr>
                <td class="d_l_t">
                    合同名称：
                </td>
                <td class="d_l_d" colspan="3">
                    <input data-options="lines:true" style="width: 200px;" id="ConName" disabled />
                </td>
            </tr>
            <tr>
                <td class="d_l_t">
                    合同事项名称：
                </td>
                <td class="d_l_d" colspan="3">
                    <input data-options="lines:true" style="width: 200px;" id="ConTemp" />
                </td>
            </tr>
            <tr>
                <td class="d_l_t">
                    事项说明：
                </td>
                <td class="d_l_t" colspan="3" style="height:60px; text-align: left;padding-left:5px;background-color:#EFEDF2">
                    <textarea id="TempRemark" class="easyui-validatebox validatebox-text" style="height:60px;  width: 97%;"></textarea>
                </td>
            </tr>
            <tr>
                <td class="d_l_t">
                    上传资料：
                </td>
                <td class="d_l_d" colspan="3">
                    <input id="file_upload" class="fileupload" name="file_upload" type="file" multiple="multiple" />
                    <div id="fileQueue" class="fileQueue">
                    </div>
                    <div id="div_files">
                    </div>
                </td>
            </tr>
        </table>
    </div>


</body>
</html>
