    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>DeviceInfo</title>
</head>
<body>
    <table width="100%" border="0" cellspacing="0" cellpadding="0" class="d_list">
        <tr>
            <th colspan="8" style="background: #efedf2; padding: 5px 0px; text-align: center;">
                <h3>
                    设备信息</h3>
            </th>
        </tr>
        <tr>
            <td align="right" class="d_l_t dtitle">
                设备编码
            </td>
            <td class="d_l_d dvalue">
                <div id="tDeviceCode">
                </div>
            </td>

            <td align="right" class="d_l_t dtitle">
                设备名称
            </td>
            <td class="d_l_d dvalue">
                <div id="tDeviceName">
                </div>
            </td>

            <td align="right" class="d_l_t">
                所属配电房
            </td>
            <td class="d_l_d">
                <div id="tPName">
                </div>
            </td>
            <td align="right" class="d_l_t">
                设备型号
            </td>
            <td class="d_l_d">
                <div id="tDeviceModel">
                </div>
            </td>
            

        </tr>
        <tr>
            <td align="right" class="d_l_t">
                生产厂家
            </td>
            <td class="d_l_d">
                <div id="tMFactory">
                </div>
            </td>
            <td align="right" class="d_l_t">
                安装地点
            </td>
            <td class="d_l_d">
                <div id="tInstallAddr">
                </div>
            </td>
            <td align="right" class="d_l_t">
                购买日期
            </td>
            <td class="d_l_d">
                <div id="tBuyTime">
                </div>
            </td>
            <td align="right" class="d_l_t">
                所属单位
            </td>
            <td class="d_l_d">
                <div id="tCompany">
                </div>
            </td>
        </tr>
        <tr>
            <td align="right" class="d_l_t">
                设备结构类型
            </td>
            <td class="d_l_d">
                <div id="tDSTID">
                </div>
            </td>
            <td align="right" class="d_l_t">
                设备类型
            </td>
            <td class="d_l_d">
                <div id="tDTID">
                </div>
            </td>
            <td align="right" class="d_l_t">
                重要程度
            </td>
            <td class="d_l_d">
                <div id="tMLID">
                </div>
            </td>
            <td align="right" class="d_l_t">
                出厂编号
            </td>
            <td class="d_l_d">
                <div id="tFactoryNumber">
                </div>
            </td>
        </tr>
        <tr>
            <td align="right" class="d_l_t">
                投运日期
            </td>
            <td class="d_l_d">
                <div id="tUseDate">
                </div>
            </td>
            <td align="right" class="d_l_t">
                使用状态
            </td>
            <td class="d_l_d">
                <div id="tUseState">
                </div>
            </td>
            <td align="right" class="d_l_t">
                建档日期
            </td>
            <td class="d_l_d">
                <div id="tBuildDate">
                </div>
            </td>
            <td align="right" class="d_l_t">
                最后维护日期
            </td>
            <td class="d_l_d">
                <div id="tLastMtcDate">
                </div>
            </td>
        </tr>
        <tr>
            <td align="right" class="d_l_t">
                备注
            </td>
            <td class="d_l_d">
                <div id="tRemarks">
                </div>
            </td>
            <td class="d_l_d" colspan="2" align="right">
                &nbsp;
            </td>
            <td align="right" class="d_l_t">
                附件
            </td>
            <td colspan="3" class="d_l_d">
                <div id="flies">
                </div>
            </td>
        </tr>
    </table>
